3 , 
CASE( MOD( DATEVALUE(Opportunity__r.Received_by_credit__c)  - Opportunity__r.Original_application_completed_date__c ,7),1,2,2,2,3,2,4,3,5,4,6,5,1), 
4 , 
CASE( MOD( DATEVALUE(Opportunity__r.Received_by_credit__c)  - Opportunity__r.Original_application_completed_date__c ,7),1,1,2,1,3,2,4,3,5,4,6,5,1), 
5 , 
CASE( MOD( DATEVALUE(Opportunity__r.Received_by_credit__c)  - Opportunity__r.Original_application_completed_date__c ,7),1,0,2,1,3,2,4,3,5,4,6,5,0), 
6 , 
