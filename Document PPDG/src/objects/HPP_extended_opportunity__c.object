        <formula>IF(ISBLANK(Date_Application_Assessed__c), 


CASE(MOD( (Date_Application_Received__c) - DATE(1985,6,24),7), 
0 , CASE( MOD( TODAY() - (Date_Application_Received__c) ,7),1,2,2,3,3,4,4,5,5,5,6,5,1), 
1 , CASE( MOD( TODAY() - (Date_Application_Received__c) ,7),1,2,2,3,3,4,4,4,5,4,6,5,1), 
