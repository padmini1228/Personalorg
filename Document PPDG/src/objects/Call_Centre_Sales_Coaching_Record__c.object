    <fields>
        <fullName>Call_Close__c</fullName>
        <externalId>false</externalId>
        <formula>IF(OR(TEXT(  Recap_call__c )=&quot;Yes&quot;, TEXT( Recap_call__c )=&quot;N/A&quot;), 2.5, 0) + 
IF(OR(TEXT(  Highlight_next_steps__c  )=&quot;Yes&quot;, TEXT(  Highlight_next_steps__c  )=&quot;N/A&quot;), 2.5, 0) + 
IF(OR(TEXT(  Confirm_customers_satisfaction__c  )=&quot;Yes&quot;, TEXT( Confirm_customers_satisfaction__c  )=&quot;N/A&quot;), 2.5, 0) + 
IF(OR(TEXT(  Recommendation__c )=&quot;Yes&quot;, TEXT( Recommendation__c )=&quot;N/A&quot;), 2.5, 0) + 
IF(OR(TEXT(  Offer_further_assistance__c  )=&quot;Yes&quot;, TEXT(  Offer_further_assistance__c  )=&quot;N/A&quot;), 2.5, 0)+
IF(OR(TEXT(  ANC_follow_up_arranged__c  )=&quot;Yes&quot;, TEXT(  ANC_follow_up_arranged__c )=&quot;N/A&quot;), 2.5, 0)+
IF(OR(TEXT(  Standard_call_close__c  )=&quot;Yes&quot;, TEXT(  Standard_call_close__c  )=&quot;N/A&quot;), 2.5, 0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Call Close</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Call_Opening_Points__c</fullName>
        <externalId>false</externalId>
        <formula>IF(OR(TEXT( Greet_the_Customer__c )=&quot;Yes&quot;, TEXT( Greet_the_Customer__c )=&quot;N/A&quot;), 5, 0) + 
IF(OR(TEXT(  Understand_reason_for_calling__c )=&quot;Yes&quot;, TEXT(  Understand_reason_for_calling__c )=&quot;N/A&quot;), 2.5, 0) + 
IF(OR(TEXT(  Collection_of_Marketing_info__c )=&quot;Yes&quot;, TEXT(  Collection_of_Marketing_info__c )=&quot;N/A&quot;), 2.5, 0) + 
IF(OR(TEXT(  ID_V__c )=&quot;Yes&quot;, TEXT( ID_V__c  )=&quot;N/A&quot;), 5, 0) + 
IF(OR(TEXT(   Understand_customers_satisfaction_with_c__c   )=&quot;Yes&quot;, TEXT(  Understand_customers_satisfaction_with_c__c )=&quot;N/A&quot;), 2.5, 0) + 
IF(OR(TEXT( Understand_what_would_make_the_customer__c  )=&quot;Yes&quot;, TEXT(  Understand_what_would_make_the_customer__c  )=&quot;N/A&quot;), 2.5, 0) + 
IF(OR(TEXT(  Summarise_needs_and_match__c  )=&quot;Yes&quot;, TEXT( Summarise_needs_and_match__c )=&quot;N/A&quot;), 2.5, 0) + 
IF(OR(TEXT(  Identify_all_selling_opportunities__c  )=&quot;Yes&quot;, TEXT( Identify_all_selling_opportunities__c )=&quot;N/A&quot;), 2.5, 0)+
IF(OR(TEXT(  Customer_ownership__c   )=&quot;Yes&quot;, TEXT(  Customer_ownership__c   )=&quot;N/A&quot;), 2.5, 0) + 
IF(OR(TEXT(  Placing_customers_on_Hold__c  )=&quot;Yes&quot;, TEXT(   Placing_customers_on_Hold__c  )=&quot;N/A&quot;), 2.5, 0) + 
IF(OR(TEXT(   Full_DPA_Disclaimers__c  )=&quot;Yes&quot;, TEXT(  Full_DPA_Disclaimers__c  )=&quot;N/A&quot;), 5, 0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Call Opening Points</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
