    <fields>
        <fullName>Variable_next_rent_review__c</fullName>
        <externalId>false</externalId>
        <formula>CASE(FLOOR((MONTH(HPPStartDate__c + CASE(MOD(HPPStartDate__c - DATE(1989,8,21),7),4,4,5,3,6,2,1))-1)/3), 
0,DATE(YEAR(HPPStartDate__c+5),3,31 - 
CASE(MOD(DATE(YEAR(HPPStartDate__c),3,31)-DATE(1989,8,21),7),5,1,6,2,0)), 
1,DATE(YEAR(HPPStartDate__c),6,30 - 
CASE(MOD(DATE(YEAR(HPPStartDate__c),6,30)-DATE(1989,8,21),7),5,1,6,2,0)), 
2,DATE(YEAR(HPPStartDate__c),9,30 - 
CASE(MOD(DATE(YEAR(HPPStartDate__c),9,30)-DATE(1989,8,21),7),5,1,6,2,0)), 
DATE(YEAR(HPPStartDate__c - CASE(MOD(HPPStartDate__c - DATE(1989,8,21),7),5,1,6,2,0)),12,31 - 
CASE(MOD(DATE(YEAR(HPPStartDate__c),12,31)-DATE(1989,8,21),7),5,1,6,2,0)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Variable next rent review</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
