        <fullName>Branch_name__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISBLANK(  Branch_code__c  ), &quot;No branch&quot;,
IF(CONTAINS(&quot;1100&quot;, Branch_code__c), &quot;Head Office&quot;,
IF(CONTAINS(&quot;1301&quot;, Branch_code__c), &quot;Manchester&quot;,
IF(CONTAINS(&quot;1101:1102&quot;, Branch_code__c), &quot;Small Heath&quot;,
IF(CONTAINS(&quot;1201:1202&quot;, Branch_code__c), &quot;Edgware Road&quot;, 
IF(CONTAINS(&quot;1210:1215&quot;, Branch_code__c), &quot;Whitechapel&quot;,
IF(CONTAINS(&quot;1120&quot;, Branch_code__c), &quot;Leicester&quot;,
IF(CONTAINS(&quot;1199&quot;, Branch_code__c), &quot;Online&quot;,
IF(CONTAINS(&quot;7261&quot;, Branch_code__c), &quot;Commercial Banking&quot;,
&quot;Other&quot;)))))))))</formula>
        <label>Branch name</label>
        <required>false</required>
