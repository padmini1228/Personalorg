    <validationRules>
        <fullName>Amber_Currency_Threshold_validation</fullName>
        <active>true</active>
        <errorConditionFormula>IF(TEXT(Metric_Type__c) == &apos;Currency&apos; &amp;&amp;  ISNULL(Amber_Currency_Threshold__c), true, false)</errorConditionFormula>
        <errorDisplayField>Amber_Currency_Threshold__c</errorDisplayField>
        <errorMessage>Amber currency threshold is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Amber_threshold_text_validation</fullName>
        <active>true</active>
        <errorConditionFormula>IF(TEXT(Metric_Type__c) == &apos;Qualitative&apos; &amp;&amp; ISNULL(Amber_Threshold_Text__c), true, false)</errorConditionFormula>
        <errorDisplayField>Amber_Threshold_Text__c</errorDisplayField>
        <errorMessage>Amber threshold text is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Amber_threshold_validation</fullName>
        <active>true</active>
        <errorConditionFormula>IF((TEXT(Metric_Type__c) == &apos;Number&apos; || TEXT(Metric_Type__c) == &apos;Percentage&apos;)&amp;&amp;  ISNULL(Amber_Threshold__c), true, false)</errorConditionFormula>
        <errorDisplayField>Amber_Threshold__c</errorDisplayField>
        <errorMessage>Amber threshold is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Green_threshold_text_validation</fullName>
        <active>true</active>
        <errorConditionFormula>IF(TEXT(Metric_Type__c) == &apos;Qualitative&apos; &amp;&amp; ISNULL(Green_Threshold_Text__c), true, false)</errorConditionFormula>
        <errorDisplayField>Green_Threshold_Text__c</errorDisplayField>
        <errorMessage>Green Threshold text is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lower_Amber_Target_Currency_validation</fullName>
        <active>true</active>
        <errorConditionFormula>IF(TEXT(Threshold_Type__c) == &apos;Lower&apos; &amp;&amp;  Target_Currency__c &gt;  Amber_Currency_Threshold__c &amp;&amp; TEXT(Type__c) == &apos;KPI&apos; &amp;&amp; TEXT(Metric_Type__c) == &apos;Currency&apos;, true, false)</errorConditionFormula>
        <errorDisplayField>Amber_Currency_Threshold__c</errorDisplayField>
        <errorMessage>Amber Currency Threshold value should always be higher than the Target Currency value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lower_amber_and_red_curr_validation</fullName>
        <active>true</active>
        <errorConditionFormula>IF(TEXT(Threshold_Type__c) == &apos;Lower&apos; &amp;&amp; Red_Currency_Threshold__c &gt; Amber_Currency_Threshold__c &amp;&amp; TEXT(Type__c) != &apos;KPI&apos;, true, false)</errorConditionFormula>
        <errorDisplayField>Red_Currency_Threshold__c</errorDisplayField>
        <errorMessage>Red Currency Threshold value should always be lower than the Amber Currency Threshold value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lower_amber_and_red_validation</fullName>
        <active>true</active>
        <errorConditionFormula>IF(TEXT(Threshold_Type__c) == &apos;Lower&apos; &amp;&amp; Red_Threshold__c &gt; Amber_Threshold__c &amp;&amp; TEXT(Type__c) != &apos;KPI&apos;, true, false)</errorConditionFormula>
        <errorDisplayField>Red_Threshold__c</errorDisplayField>
        <errorMessage>Red Threshold value should always be lower than the Amber Threshold value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lower_amber_and_target_validation</fullName>
        <active>true</active>
        <errorConditionFormula>IF(TEXT(Threshold_Type__c) == &apos;Lower&apos; &amp;&amp;  Target__c &gt; Amber_Threshold__c &amp;&amp; TEXT(Type__c) == &apos;KPI&apos; &amp;&amp; (TEXT(Metric_Type__c) == &apos;Percentage&apos; || TEXT(Metric_Type__c) == &apos;Number&apos;), true, false)</errorConditionFormula>
        <errorDisplayField>Amber_Threshold__c</errorDisplayField>
        <errorMessage>Amber Threshold value should always be higher than the Target value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lower_yellow_and_amber_curr_validation</fullName>
        <active>true</active>
        <errorConditionFormula>IF(TEXT(Threshold_Type__c) == &apos;Lower&apos; &amp;&amp; Amber_Currency_Threshold__c &gt; Yellow_Currency_Threshold__c &amp;&amp;  Use_Yellow_Threshold__c, true, false)</errorConditionFormula>
