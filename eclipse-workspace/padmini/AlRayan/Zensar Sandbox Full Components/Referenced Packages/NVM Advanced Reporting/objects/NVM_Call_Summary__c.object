        <fullName>Fiscal_Quarter__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>CASE(MONTH(datevalue(  CallTime__c  )) , 
1, &quot;Q1-&quot;+ TEXT(year(datevalue(CallTime__c))), 
2, &quot;Q1-&quot;+ TEXT(year(datevalue(CallTime__c))), 
