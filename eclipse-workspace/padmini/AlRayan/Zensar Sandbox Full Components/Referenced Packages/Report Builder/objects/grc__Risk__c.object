    <fields>
        <fullName>Cost_Most_Likely__c</fullName>
        <deprecated>false</deprecated>
        <description>This field displays the inherent cost most likely value if there&apos;s no residual cost most likely value.</description>
        <externalId>false</externalId>
        <formula>IF(Risk__Cost_Most_Likely_Case__c==null||Risk__Cost_Most_Likely_Case__c==0,Risk__Inherent_Cost_Most_Likely_Case__c,Risk__Cost_Most_Likely_Case__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>This field displays the most likely financial exposure based on latest residual assessment. If residual assessment has not been performed for the risk then the value is based on latest Inherent assessment.</inlineHelpText>
        <label>Cost Most Likely</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
